	// 			width: 100px;
	// 			right: auto;
	// 			left: 40%;
	// 		}
	// 	}
	// }

	.footer-logo-brand {
		width: 150px;
		img {
			width: 100%;
		}
	}
}

@media only screen and (max-width: 359px) {
	#imageObject3 {
		img {
			&:nth-child(2) {
