#imageObject1 {
	max-width: 450px;
	width: 100%;
	padding-left: 50px;
	padding-right: 50px;
	img {
		&:nth-child(1) {
			width: 100%;
		}
		&:nth-child(2),
		&:nth-child(3),
		&:nth-child(4) {
			position: absolute;
		}
		&:nth-child(2) {
			left: 0;
			top: 50%;
		}
		&:nth-child(3) {
			left: 0;
			bottom: 10px;
		}
		&:nth-child(4) {
			right: 0;
			top: 40%;
		}
	}
}

#imageObject2 {
	max-width: 450px;
	width: 100%;
	padding-left: 50px;
	padding-right: 50px;
	img {
		&:nth-child(1) {
			width: 100%;
			z-index: 10;
			position: relative;
		}
		&:nth-child(2),
		&:nth-child(3),
		&:nth-child(4),
		&:nth-child(5),
		&:nth-child(6) {
			position: absolute;
		}
		&:nth-child(2) {
			width: 100px;
			top: 150px;
			right: 70px;
			z-index: 9;
		}
		&:nth-child(3) {
			width: 270px;
			top: 80px;
			left: 80px;
			z-index: 8;
		}
		&:nth-child(4) {
			width: 75px;
			bottom: 120px;
			left: 80px;
			z-index: 7;
		}
		&:nth-child(5) {
			width: 200px;
			right: 0;
			bottom: 0;
		}
		&:nth-child(6) {
			width: 30px;
			right: 0;
			bottom: 50px;
		}
	}
}

#imageObject3 {
	max-width: 400px;
	width: 100%;
	padding-right: 200px;
	img {
		&:nth-child(1) {
			width: 100%;
			animation: 2s wiggle infinite;
		}
		&:nth-child(2) {
			width: 200px;
			position: absolute;
			right: 0;
			top: 0;
		}
	}
}

#imageObject4 {
	margin-bottom: -70px;
	img {
		&:nth-child(1) {
			position: absolute;
			left: -20px;
			top: 50px;
			z-index: 1;
		}
		&:nth-child(2) {
			width: 100%;
			position: relative;
			z-index: 2;
		}
		&:nth-child(3) {
			width: 40px;
			position: absolute;
			right: 23%;
			bottom: 31%;
			z-index: 3;
			animation: 1s headbang infinite;
		}
	}
}




/*===========================================================================
0.3 Footer
===========================================================================*/

footer {
	background-color: $color-blood;
	color: white;
	padding: 20px;
}

.footer-row {
	&::after {
		content: '';
		display: block;
		clear: both;
	}
}

.footer-left {
	float: left;
}

.footer-right {
	float: right;
	text-align: right;
}

.footer-logo {
	margin-bottom: 10px;
}




/*===========================================================================
X.X Media Queries
===========================================================================*/

@media only screen and (max-width: 1079px) {
	#what {
		background-color: $color-naruto;
		background-image: none !important;
	}
}

@media only screen and (max-width: 479px) {
	.header-logo {
		width: 150px;
		img {
			width: 100%;
		}
	}

	#imageObject1 {
		img {
			&:nth-child(3) {
				width: 70px;
			}
			&:nth-child(4) {
				width: 100px;
			}
		}
	}

	#imageObject2 {
		max-width: 300px;
		margin-top: 60px;
		img {
			&:nth-child(2) {
				width: 50px;
				top: 75px;
				right: 65px;
			}
			&:nth-child(3) {
				width: 150px;
				top: 0px;
				left: 60px;
			}
			&:nth-child(4) {
				width: 32px;
				bottom: 70px;
				left: 60px;
			}
			&:nth-child(5) {
				width: 100px;
				right: 0;
				bottom: 0;
			}
			&:nth-child(6) {
				width: 15px;
				right: 0;
				bottom: 25px;
			}
		}
	}

	// #imageObject3 {
	// 	img {
